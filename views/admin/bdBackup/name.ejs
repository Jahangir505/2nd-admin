
<!--=========================-->

<ol class="breadcrumb">
  <li class="breadcrumb-item">
    <a href="<%=link1%>"><%=title%></a>
  </li>
  <li class="breadcrumb-item active"><%=subtitle%></li>
</ol>
<div class="row">
  <div class="col-12">
    <div class="card-header">
      <i class="fa fa-table"></i> <%=title%>
    </div>

    <div class="card-body" >
      <div class="container">

        <% if(flashMsgError && flashMsgError.length > 0 ) { %>
          <div class="alert alert-danger">
            <% if(flashMsgError.length === 1) { %>
              <strong> <%= flashMsgError %> </strong>
            <% } else { %>
              <ul>
                <% flashMsgError.forEach(function(err) { %>
                  <li> <strong> <%= err %> </strong></li>
                <% }) %>
              </ul>
            <% } %>
          </div>
        <% } %>

        <% if(flashMsgSuccess && flashMsgSuccess.length > 0 ) { %>
          <div class="alert alert-success">
            <% if(flashMsgSuccess.length === 1) { %>
              <strong> <%= flashMsgSuccess %> </strong>
            <% } else { %>
              <ul>
                <% flashMsgSuccess.forEach(function(err) { %>
                  <li> <strong> <%= err %> </strong></li>
                <% }) %>
              </ul>
            <% } %>
          </div>
        <% } %>

      </div>

      <form class="form-vertical" role="form" action=<%=formActionTarget%> method="GET" enctype="multipart/form-data">

        <div class="form-group row">
          <div class="col-md-6">
            <label for="name">Name</label>
            <input type="text" class="form-control" name="name" id="name" placeholder="Name" value="<%=data.name%>">
            <input type="hidden" class="form-control" name="fromWeb" id="name"  value="fromWeb">
          </div>


        </div>


        <div class="form-group">
          <button ng-click="abc()" type="submit" class="btn btn-primary">Submit</button>
        </div>

      </form>
      <div class="clearfix"></div>
    </div>

  </div>
</div>


<!--=========================-->
